{
    "name": "luislira-videos",
    "version": 2,
    "builds": [{
        "src": "index.js",
        "use": "@now/node"
    }],
    "routes": [{
        "src": "/(.*)",
        "dest": "/index.js"
    }],
    "env": {
        "DB_USER": "llv-user",
        "DB_PASSWORD": "llv-password",
        "DB_HOST": "llv-host",
        "DB_NAME": "llv-dbname"
    }
}